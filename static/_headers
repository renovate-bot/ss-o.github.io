# Folders which are safe for immutable caching, as filename change when content change

/assets/*
  Cache-Control: public
  Cache-Control: max-age=365000000
  Cache-Control: immutable

/fonts/*
  Cache-Control: public
  Cache-Control: max-age=365000000
  Cache-Control: immutable

/img/*
  Cache-Control: public
  Cache-Control: max-age=365000000
  Cache-Control: immutable
